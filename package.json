{
  "name": "lora-style-transfer-trae",
  "version": "2.0.0",
  "private": true,
  "description": "AI-powered LoRA style transfer application built with Next.js and Convex on the Macaly platform",
  "author": "Marek Hospodarsky <mailhospodarsky@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mulenmara1505/lora-style-transfer.git"
  },
  "keywords": [
    "ai",
    "lora",
    "style-transfer",
    "neural-networks",
    "image-processing",
    "art",
    "nextjs",
    "convex",
    "macaly"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:full": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" --names \"NEXT,PY\" --prefix-colors \"blue,green\"",
    "dev:frontend": "next dev",
    "dev:backend": "cd backend && python3 main.py",
    "dev:frontend-only": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "convex:dev": "npx convex dev",
    "convex:deploy": "npx convex deploy",
    "test": "echo 'Tests will be implemented in future versions'",
    "clean": "rm -rf .next node_modules/.cache"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.487.0",
    "next": "14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.15",
    "jsdom": "^26.0.0",
    "macaly-tagger": "^1.0.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
